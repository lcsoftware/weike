@charset "utf-8";
/* CSS Document */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,select,textarea,p,blockquote,th,td,hr,button,figure,menu{margin:0;padding:0;}
body{font:12px/22px Tahoma,Arial,\5b8b\4f53,sans-serif;}
h1,h2,h3,h4,h5,h6{font-size:100%;}
table{border-collapse:collapse;border-spacing:0;}
ol,ul{list-style:none;}
a img{border:none; vertical-align:top;}
input,select{ font-family:'Microsoft YaHei';}
a{ text-decoration:none;}a:hover{ text-decoration: none;}
body{ font-family:'Microsoft YaHei';}

/*公用图片icon样式*/
.icon{ display:inline-block; width:16px; height:16px; background:url(/images/icon_index.png) no-repeat;}
.icon_24{ display:inline-block; width:24px; height:24px; background:url(/images/icon_24.png) no-repeat;}
.backto_top{ display:block; width:40px; height:40px; position:fixed; bottom:60px; left:50%; margin-left:610px; background-position:-69px -40px;}

/*框架*/
.wrap{ background:#95a3c0; position:relative;}
.main_bg{ position:relative;}
.main_all{ width:1200px; min-height:800px; overflow:hidden; zoom:1; margin:15px auto 0; position:relative;}
.main_content{ float:left; overflow:hidden; zoom:1; width:1000px;margin-bottom:-10000px; padding-bottom:10000px; background:#fff; position:relative;}

/*右侧头部公告*/
.right_header{ height:40px; line-height:40px; background:#fff; overflow:hidden; zoom:1;}
.freshman{ float:right; margin-right:30px; position:relative; padding-left:20px;}
.freshman a{ font-size:12px;color:#4a596e;}
.freshman a:hover{ text-decoration:underline;}
.freshman i{ background-position:0 -84px; position:absolute; left:0; top:13px;}
.notice_box{ float:left; width:750px; overflow:hidden; zoom:1;}
.notice_list{ float:left; overflow:hidden; zoom:1; margin-left:25px;}
.notice_list li{ display:none; width:640px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; font-size:12px; color:#666; padding-left:30px; position:relative;}
.notice_list li i{ background-position:0 -34px; position:absolute; left:0; top:13px;}
.notice_btn{ float:right; margin:7px 0 0 10px;}
.notice_btn a{ display:block; float:left; margin-left:1px; width:21px; height:26px;}
.notice_btn a.prev_btn{ background-position:-16px 0;}
.notice_btn a.prev_btn:hover{ background-position:-16px -52px;}
.notice_btn a.next_btn{ background-position:-16px -26px;}
.notice_btn a.next_btn:hover{ background-position:-16px -78px;}
.coursesite_box{font-family:'Microsoft YaHei'; padding-bottom:20px;}
.coursesite_box h4{ padding-left:30px; height:40px; line-height:40px; background:#374760; font-size:16px;  color:#fff;}

/*展开收起左侧内容*/
.icon_fold{ display:block; width:30px; height:30px; background:url(../images/fold.gif) no-repeat; position:absolute; left:50%; margin-left:-430px; top:68px; z-index:999;}
.icon_fold:hover{background:url(../images/fold_hover.gif) no-repeat;}


/*翻页*/
.page_box{ margin-top:10px; overflow:hidden; zoom:1; text-align:center;}
.page_box a{ display:inline-block; height:25px; color:#666; line-height:25px; padding:0 6px; border:1px solid #cbcbcb;}
.page_box a.active,.page_box a:hover,.page_box a.active{ background:#374760; border:1px solid #2a3950; color:#fff;}
.page_box span{ color:#999; padding:0 5px;}
.page_box span input{ width:30px; height:20px; text-align:center; line-height:20px\9; border:1px solid #ccc; margin:0 5px;}

/*保存，取消按钮*/
.btn_box{ margin-top:20px; text-align:center; overflow:hidden; zoom:1; clear:both;}
.btn_box a{ display:inline-block; margin-right:10px; padding:0 25px; height:32px; line-height:32px; text-align:center; border:1px solid #c9c9c9; font-size:14px; color:#666;}
.btn_box a.save{ background:#475c7c; color:#fff;}
.btn_box a.save:hover{ background:#3f516d;}
.btn_box a.cancel{ background:#f2f2f2; color:#666;}
.btn_box a.cancel:hover{ background:#e2dfdf;}
.btn_box a.keep{ background:#cc0000; color:#fff;}
.btn_box a.keep:hover{ background:#b40909;}

/*教学班表格列表*/
.search_content .class_box{ margin-top:10px;}
.class_box{ width:100%; border:1px solid #cbcbcb; background:#fff;}
.class_box tr th{ height:30px; background:#cccccc; font-size:13px; color:#fff; font-weight:normal; text-align:left; padding-left:5px;}
.class_box input[type=checkbox]{height:30px; vertical-align:middle; margin-right:3px;}
.class_box tr.active td{ background:#374760; color:#fff;}
.class_box tr.active td.has_border{ background:#374760;}
.class_box tr.active .open_btn{ color:#fff; text-decoration:underline;}
.class_box tr.active td i{ display:block;}
.class_box tr td{ height:30px; line-height:30px; border-top:1px solid #cbcbcb; margin-top:-1px; overflow:hidden; zoom:1; color:#666; padding-left:5px; position:relative;}
.class_box tr td.has_border,.class_box tr th.has_border{ padding-left:7px;}
.class_box tr td.has_border{ background:#f2f2f2;}

/*教学班表格列表按钮*/
