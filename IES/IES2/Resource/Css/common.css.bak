/*教学班表格列表操作按钮*/
.open_btn{ color:#1e5975; text-decoration:underline;}
.btn_list{ float:right; margin-right:10px; overflow:hidden; zoom:1;}
.btn_list li{ width:16px; height:16px; float:left; margin:7px 5px 7px 0; cursor:pointer;}
.icon_import{ background-position:-53px -32px;}
.icon_import:hover{ background:url(../images/icon_construction.png) no-repeat -64px 0;}
.icon_export{background-position:-53px -48px;}
.icon_export:hover{ background:url(../images/icon_construction.png) no-repeat -48px -80px;}
.icon_edit{ background-position:0 -187px;}
.icon_edit:hover{ background-position:0 -169px;}
.icon_delete{background-position:-53px -80px;}
.icon_delete:hover{ background-position:-37px -128px;}

/*右侧头部公告以下两个部分*/
.exercise_search{ padding:10px 0; background:#f2f2f2; height:50px;border-bottom:1px solid #ccc;}
.select_condition{ float:left; margin-left:25px;}
.select_condition select{ display:block; float:left; margin-right:20px; height:25px; line-height:25px\9; background:#fff; border:1px solid #e4e4e4; font-size:12px; color:#666;}
.current_course{ color:#666; padding-left:35px; position:relative; height:25px;}
.current_course i{ display:block; width:24px; height:24px; background-position:-72px -168px; position:absolute; top:0; left:0;}
.exercise_btn{ float:right; margin-right:18px;}
.exercise_btn a{ display:block; float:right; margin-left:10px; width:105px; height:25px; line-height:25px; background:url(../images/add_bg.gif) no-repeat; color:#fff; position:relative;}
.exercise_btn a:hover{ background:url(../images/add_bg_hover.gif) no-repeat;}
.exercise_btn a em{ display:block; width:25px; height:25px; vertical-align:middle; float:left;}
.exercise_btn a em i{ display:block; width:16px; height:16px; background-position:-53px 0; margin:4px 0 0 4px;}
.exercise_btn a span{ display:block; float:left; width:80px; text-align:center;}
.sort_box{ float:right; margin-left:20px; color:#475c7c; text-decoration:underline; cursor:pointer;}
.batch_list{ display:none; position:absolute; top:25px; left:25px; z-index:10;}
.batch_list li{ width:80px; height:22px; color:#fff; text-align:center; line-height:22px; background:#475c7c; position:relative;}
.permissions{ display:none; position:absolute; top:2px; left:81px;}
.permissions li{ width:auto; height:auto; color:#333; line-height:30px; text-align:left; background:#fff;}
.permissions li p{ width:126px; padding-left:32px; height:30px; line-height:30px;border-bottom:0; border:1px solid #ddd;  position:relative; margin-top:-1px; overflow:hidden; zoom:1;}
.batch_list li.active li.current{ background:#475c7c; color:#fff;}
.permissions li p i{ display:block; width:16px; height:16px; background-position:-69px -160px; position:absolute; top:7px; left:8px;}
.batch_list li.active{ background:#374760;}
.batch_list li.active .permissions{ display:block;}
.search_topic{ float:right; position:relative; }
.search_topic input{width:170px; padding:0 25px 0 5px; background:#fff; height:23px\9; line-height:23px; border:1px solid #999999; font-size:12px; color:#666;}
.search_topic span{ display:block; width:16px; height:16px; background-position:-37px -144px; position:absolute; right:5px; top:4px; cursor:pointer;}

.exercise_nav{ height:54px; background:#374760; position:relative;}
.scan_more{ display:block; width:230px; height:44px; line-height:44px; text-align:center; position:absolute; right:25px; top:10px; background:#475c7c;font-size:13px; color:#fff;}
.scan_more i{  margin-left:5px; vertical-align:text-bottom;background-position:0 0;}
.exercise_nav_list{ padding:10px 0 0 25px; height:44px;}
.exercise_nav_list li{ float:left; margin-right:10px; width:230px; background:#475c7c; position:relative;}
.first_nav{ display:block; margin:0 10px;  overflow:hidden; text-overflow:ellipsis; white-space:nowrap; height:44px; line-height:44px; text-align:center; font-size:13px; color:#fff;}
.exercise_nav_list li:hover,.scan_more:hover{ background:#5e769b;}
.second_nav a:hover{background:#5e769b;}
.first_nav i{ background-position:0 0; vertical-align:text-bottom; margin-left:5px;}
.first_nav i.slide_up{ background-position:0 -16px;}
.second_nav{ display:none; position:absolute; top:44px; right:0; z-index:12;}
.second_nav a{ display:block; width:300px; height:44px; line-height:44px; background:#475c7c; margin-top:1px; color:#fff; text-align:center; font-size:14px;}
.exercise_nav_list li.active{background:#f2f2f2; }
.exercise_nav_list li.active .first_nav{ color:#374760;}
.exercise_box{ overflow:hidden; zoom:1;}


/*资料条件筛选*/
.filter_box{ padding:15px 35px 5px; border-top:1px solid #cccccc; border-bottom:1px solid #cccccc; background:#e4e4e4; position:relative;}
.filter_list{ margin-bottom:10px; overflow:hidden; zoom:1;}
.filter_list dt{ float:left; width:50px; color:#666; margin-right:15px;}
.filter_list dd{ overflow:hidden; zoom:1;}
.filter_list dd a{ display:block; float:left; margin-right:3px; margin-bottom:3px; color:#666;}
.filter_list dd a span{ display:block; height:27px; line-height:27px; padding:0 15px; min-width:20px;}
.filter_list dd a.active span,.filter_list dd a:hover span{ color:#fff; background:#475c7c;}
.guanbi{ display:block; width:24px; height:24px;background-color: rgba(0, 0, 0, 0.4); filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#88000000, endColorstr=#88000000); position:absolute; right:0; top:0;}
.guanbi i{ display:block; width:16px; height:16px; background-position:-37px -160px; margin:5px 0 0 5px;}

/*资料表格列表*/
.course_data{ width:100%; border-bottom:1px solid #cccccc;color:#666;}
.course_data tr th{ height:45px; line-height:45px; text-align:left; padding-left:10px; font-weight:normal; overflow:hidden; }
.course_data tr td{ line-height:45px; padding-left:10px; border-top:1px solid #ccc; position:relative;}
.data_tit{ width:200px; float:left; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;} 
.data_name{ height:45px; line-height:45px;}
.data_name input{ display:block; float:left; margin-right:10px; height:45px; vertical-align:middle;}
.data_name p{ float:left;}
.data_name i{ display:block; float:left; cursor:pointer;}
.data_name i.icon_24{ width:24px; height:24px; margin:10px 5px 0 0;}
.arrow{ width:4px; height:12px; background-position:-95px -131px; margin:16px 0 0 5px;}
.data_name input.name_file{ width:200px; height:16px; line-height:16px; border:1px solid #cccccc; margin-top:13px; color:#666;}
/*资料格式*/
.file{background-position:-96px -72px;}
.doc{ background-position:-96px 0;}
.mp4{ background-position:-96px -24px;}
.mp3{background-position:-96px -48px;}
.flv{background-position:-96px -96px;}
.ppt{background-position:-96px -120px;}
.iso{background-position:-96px -144px;}
.rar{background-position:-96px -168px;}
.zip{background-position:-96px -192px;}
.pdf{ background-position:0 -96px;}
.psd{ background-position:0 -120px;}
.txt{ background-position:0 -144px;}
.excel{ background-position:0 -168px;}
.html{ background-position:0 -192px;}
.paper{ background-position:-72px -192px;}

/*弹出层*/
.pop_bg{ width:100%; height:100%; position:absolute; top:0; left:0;background-color: rgba(0, 0, 0, 0.4); filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#88000000, endColorstr=#88000000); z-index:998;}
.pop_600{ display:none;width:600px; position:absolute; left:50%; margin-left:-300px; top:200px; background:#fff; z-index:999; }
.pop_600 h4{ height:40px; line-height:40px; background:#1e5975; font-size:14px; color:#fff; padding-left:13px;}
.icon_close{ background-position:-37px -128px; position:absolute; right:12px; top:12px; cursor:pointer;}
.close_pop{ background-position:-37px -128px; position:absolute; right:12px; top:12px; cursor:pointer;}
.pop_400{  width:400px; position:absolute; left:50%; margin-left:-200px; top:200px; background:#fff; z-index:999;}
.pop_400 h4{ height:40px; line-height:40px; background:#1e5975; font-size:14px; color:#fff; padding-left:13px;}
.pop_800{ display:none; width:800px; position:absolute; top:200px; left:50%; margin-left:-400px; z-index:999; background:#fff;}
.pop_800 h4{ height:40px; line-height:40px; background:#1e5975; font-size:14px; color:#fff; padding-left:13px;}
.pop_1000{ display:none; width:1000px; position:absolute; top:200px; left:50%; margin-left:-400px; z-index:999; background:#fff;}
.pop_1000 h4{ height:40px; line-height:40px; background:#1e5975; font-size:14px; color:#fff; padding-left:13px;}
.pop_wrap{ padding:20px; border:1px solid #ccc;}


.pre{margin:0 0 10px;padding:0}
