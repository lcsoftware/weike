<div class="paper_content">    
    <input type="hidden" name="editorInput" id="editorInput" ng-model="editorText">
    <iframe id="editor01" src="Frameworks/eWebEditor/ewebeditor.htm?id=editorInput&style=coolblue"
            frameborder="0" scrolling="no" width="850" height="300" style="display: block;"></iframe>
    <a class="add_attach" dialog-show dialog-id="eIesUploader"><em><i class="icon"></i></em><span>添加附件</span></a>
    <attach-list attachments="attachmentList"></attach-list>
    <p class="no_rank">翻译题答案可以为空</p>
    <div class="text_box" style="color:#F00;">
        <input type="hidden" name="editorAnswer" id="editorAnswer" ng-model="editorAnswerText" ng-value="editorAnswerText">
        <iframe src="Frameworks/eWebEditor/ewebeditor.htm?id=editorAnswer&style=coolblue"
                frameborder="0" scrolling="no" width="880" height="300" style="display: block;"></iframe>
    </div>
    <br />
    <div class="text_box" style="color:#F00;">
        <input type="hidden" name="editorAnalysis" id="editorAnalysis" ng-model="editorAnalysisText" ng-value="editorAnalysisText">
        <iframe src="Frameworks/eWebEditor/ewebeditor.htm?id=editorAnalysis&style=coolblue"
                frameborder="0" scrolling="no" width="880" height="300" style="display: block;"></iframe>
    </div>
</div>
<ies-file-uploader id="eIesUploader" style="display:none" file-catetory="2"></ies-file-uploader>